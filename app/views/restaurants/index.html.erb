<div class="container">
  <h1>Restaurants</h1>
  <ul>
    <% @restaurants.each do |restaurant| %>
      <li>
        <%= link_to restaurant.name, restaurant_path(restaurant) %>
      </li>
    <% end %>
  </ul>
  <br>
  <p class="btn btn-primary"><%= link_to "Add a new Restaurant", new_restaurant_path %></p>
</div>
